DATABASE_URL=postgres://user:password@localhost:5432/ragtime?sslmode=disable

# Object store
OBJECT_STORE_TYPE=s3
OBJECT_STORE_ROOT=/tmp/ragtime-objects
REGION=us-east-1
BUCKET_NAME=ragtime-bucket
ENDPOINT_URL=https://s3.amazonaws.com
ACCESS_KEY_ID=your-access-key
SECRET_ACCESS_KEY=your-secret-key
S3_FORCE_PATH_STYLE=false

# Embeddings
EMBEDDING_PROVIDER=fastapi
EMBEDDING_BASE_URL=http://localhost:8001
FASTAPI_EMBEDDINGS_URL=http://localhost:8001
OPENAI_API_KEY=sk-your-key
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIMENSIONS=1536
EMBEDDING_MODEL_ID=text-embedding-3-small
EMBEDDING_MODEL_VERSION=2025-01-01

# Logging
LOG_LEVEL=info
