# Required: deployed Postgres/PGVector connection string
DATABASE_URL=postgres://user:password@host:5432/dbname?sslmode=require

# Local object storage in the backend container
OBJECT_STORE_TYPE=local
OBJECT_STORE_ROOT=/data/objects

# Embedding client config
# Keep this aligned with whichever embedding provider your backend should call.
EMBEDDING_PROVIDER=fastapi
EMBEDDING_BASE_URL=http://embeddings:8000
EMBEDDING_MODEL_ID=text-embedding-3-small

# Optional logging level
LOG_LEVEL=info
